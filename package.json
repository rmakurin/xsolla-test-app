    "css-loader": "^3.6.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "^4.3.0",
    "html-loader": "^0.5.5",
    "mini-css-extract-plugin": "^0.7.0",
    "nodemon": "^1.19.4",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "typescript": "^3.9.7",
    "url-loader": "^2.3.0",
